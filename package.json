{
  "name": "node-ts-init",
  "version": "1.0.0",
  "description": "A template for a Node.js project with TypeScript, eslint and prettier",
  "main": "index.js",
  "type": "module",
  "engines": {
    "node": ">=20.6.0"
  },
  "scripts": {
    "dev": "node --loader=ts-node/esm --env-file=.env.local --watch src/index.ts",
    "build": "tsc -p .",
    "start": "node build/index.js",
    "lint": "eslint src"
  },
  "author": "Andreas Ã–stling",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.0.1",
    "express": "^4.21.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.2",
    "@typescript-eslint/eslint-plugin": "^8.18.0",
    "@typescript-eslint/parser": "^8.18.0",
    "eslint": "^9.17.0",
    "globals": "^15.13.0",
    "prettier": "^3.4.2",
    "prisma": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.18.0"
  }
}
